 <!-- ==================== Start Contact ==================== -->
 <section class="about-ar section-padding">
	<div class="container">
		<div class="col-lg-8 col-md-10">
			<div class="sec-head  text-center">
				<h2 class="wow fadeIn" data-wow-delay=".3s">Edit   <span class="color-font">Profile</span></h2>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-4 valign">
				<div class="img">
					<img src="http://localhost:3030/vie-dark/img/user.png" >
					<div class="row">
						<div class="col-12">
							<div class="text-center">
								<input type="submit" class="butn light mt-30 full-width" value = "Change your profile" >
								</div>
						</div>
					</div>
				</div>
			</div>

			<div class="col-lg-8 valign">

				<section class="contact-sec style2 section-padding position-re bg-img" data-background="http://localhost:3030/vie-dark/img/patrn1.png" data-scroll-index="7">
					<div class="container">
						<div class="row justify-content-center">
							<div class="col-lg-10">

								<div class="form wow fadeInUp" data-wow-delay=".5s">

									<form  method="post" action="/edit_user">
			
										<div class="controls">

                                            <div class="row">
                                                <input id="user_id" type="hidden" name="user_id" value="<%=user._id%>"">
												<div class="col-lg-12">
													<div class="form-group">
														<h6>First Name</h6>
														<input id="first_name" type="text" name="first_name" value="<%=user.first_name%>""
														>
													</div>
												</div>
												<div class="col-lg-12">
													<div class="form-group">
														<h6>Last Name</h6>
														<input id="last_name" type="text" name="last_name" value="<%=user.last_name%>""
														>
													</div>
												</div>
												<div class="col-lg-12">
													<div class="form-group">
														<h6>Email</h6>
														<input id="Email" type="text" name="Email" value="<%=user.email%>"
														>
													</div>
												</div>
												<div class="col-lg-12">
													<div class="form-group">
														<h6>Phone</h6>
														<input id="Phone" type="tel" name="Phone" value="<%=user.phone%>"
															>
													</div>
													<div class="col-12">
														<div class="text-center">
														<input type="submit" class="butn light mt-30 full-width" value = "Change" >
														</div>
													</div>
												</div>
											</div> 
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</section>
			
			</div>
		</div>
	</div>
</section>

	</div>
</section>

