<!DOCTYPE html>
<html lang="zxx">

<head>

    <!-- Metas -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="keywords" content="HTML5 Template Vie onepage themeforest" />
    <meta name="description" content="Vie - Onepage Multi-Purpose HTML5 Template" />
    <meta name="author" content="" />

    <!-- Title  -->
    <title>Bidlersegde</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="http://localhost:3030/vie-dark/img/auc_icon.png" />

     <!-- Google Fonts -->
     <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900&display=swap"
     rel="stylesheet">
    <link
     href="https://fonts.googleapis.com/css2?family=Montserrat+Alternates:wght@100;200;300;400;500;600;700;800;900&display=swap"
     rel="stylesheet">


    <!-- Plugins -->
    <link rel="stylesheet" href="http://localhost:3030/vie-dark/css/plugins.css" />

    <!-- Core Style Css -->
    <link rel="stylesheet" href="http://localhost:3030/vie-dark/css/style.css" />

</head>

<body class="position-r">



    <!-- ==================== Start Loading ==================== -->

    <div class="loading">
        <span>L</span>
        <span>o</span>
        <span>a</span>
        <span>d</span>
        <span>i</span>
        <span>n</span>
        <span>g</span>
    </div>

    <div id="preloader">
    </div>

    <!-- ==================== End Loading ==================== -->

    

    <!-- ==================== Start cursor ==================== -->

    <div class="mouse-cursor cursor-outer"></div>
    <div class="mouse-cursor cursor-inner"></div>

    <!-- ==================== End cursor ==================== -->






    <div class="main-content">


 <!-- ==================== Start About ==================== -->

 <section class="about-ar section-padding">
	<div class="container">
		<div class="row">
			<div class="col-lg-4 valign">
				<div class="img">
					<div class="bg-img bg-fixed hero-bg" data-background="img/arch/hero.jpg"></div>
					<div class="exp valign text-center">
						<div class="full-width">
							<h1 class="wow fadeIn" data-wow-delay=".3s">Reserve Room</h1>
						</div>
					</div>
				</div>
			</div>

			<div class="col-lg-8 valign">

				<section class="contact-sec style2 section-padding position-re bg-img" data-background="img/patrn1.png" data-scroll-index="7">
					<div class="container">
						<div class="row justify-content-center">
							<div class="col-lg-6 col-md-6">
							</div>
						</div>
						<div class="row justify-content-center">
							<div class="col-lg-10">

								<div class="form wow fadeInUp" data-wow-delay=".5s">

									<form  method="post" action="/edit_room">
			
										<div class="controls">

                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <h6>Room</h6>
                                                    <input  type="hidden" name="reserve_id" value="<%=room._id%>">
                                                    <input  id="user_id" type="hidden" name="user_id" value="<%=user._id%>"">
                                                </div>
                                            </div>

                                            <div class="row">
												<div class="col-lg-6">
													<div class="form-group">
														<h6>Time Open</h6>
														<input id="time_open" type="time" name="time_open" value="<%=room.time_open%>"
														>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group">
														<h6>Time Close</h6>
														<input id="time_close_door" type="time" name="time_close_door" value="<%=room.time_close_door%>"
														>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group">
														<h6>Time Finish</h6>
														<input id="time_finish" type="time" name="time_finish" value="<%=room.time_finish%>"
														>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group">
														<h6>Date</h6>
														<input id="auction_day" type="date" name="auction_day" value="<%=room.auction_day%>"
															>
													</div>
												</div>
											</div> 
										</div>
                                        <div class="controls">
			
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <h6>Item</h6>
                                                    <input  type="hidden" name="" value=""
                                                    >
                                                </div>
                                            </div>

                                            <div class="row">
												
                                                <div class="col-lg-6">
													<div class="form-group">
														<h6>Advertising words</h6>
														<input  type="text" name="item_ad" id="item_ad" placeholder="Type Advertising Words">
													</div>
													<div class="text-center">
                                                         <select name="item_id" id="item_id" class="butn white mt-30 full-width">
                                                            <option value="">-- Select an item --</option>
                                                            <% Object.values(item).forEach(function(item){%>
                                                            <option value ="<%= item._id%>" name="item_id"><%= item.item_name%></option>
                                                            <% }) %>
                                                          </select>
													</div>
												</div>
												<div class="col-lg-12">
													<div class="text-center">
														<input type="submit" name="submit" id="submit" class="butn light mt-30 full-width" value = "Reserve" >
													</div>
												</div>
											</div> 
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</section>
			
			</div>
		</div>
	</div>
</section>

<!-- ==================== End About ==================== -->






    </div>




    <!-- jQuery -->
    <script src="http://localhost:3030/vie-dark/js/jquery-3.0.0.min.js"></script>
    <script src="http://localhost:3030/vie-dark/js/jquery-migrate-3.0.0.min.js"></script>

    <!-- plugins -->
    <script src="http://localhost:3030/vie-dark/js/plugins.js"></script>

    <!-- custom scripts -->
    <script src="http://localhost:3030/vie-dark/js/scripts.js"></script>

</body>

</html>