                    <a class="nav-link btn-custom1" href="/login">Book Rooms</a>
                  </li>
                </ul>
              </div>
            </div>
          </nav>
        </div>
    <!-- Room -->
<% Object.values(room).forEach(function(room){%>
    <div class ='col-lg-4 col-md-6 mb-4'>
      <div class="card h-100">
        <a href="#"></a>
        <div class="'card-body">
          <h4 class="card-title">
              <a href="/<%= room._id%>"> Time open: <%= room.time_open%></a>
          </h4>
          <h5>Time open: <%= room.time_open%></h5>
          <h6>Time close: <%= room.time_close_door%></h6>
          <h7>Time finish: <%= room.time_finish%></h7>
          <h8>Date: <%= room.auction_day%></h8>
        </div>
      </div>
    </div>
<% }) %>
