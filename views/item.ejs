    <!-- Title  -->
    <title>Bidlersegde</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="http://localhost:3030/vie-dark/img/auc_icon.png" />

     <!-- Google Fonts -->
     <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900&display=swap"
     rel="stylesheet">
    <link
     href="https://fonts.googleapis.com/css2?family=Montserrat+Alternates:wght@100;200;300;400;500;600;700;800;900&display=swap"
     rel="stylesheet">


    <!-- Plugins -->
    <link rel="stylesheet" href="http://localhost:3030/vie-dark/css/plugins.css" />

    <!-- Core Style Css -->
    <link rel="stylesheet" href="http://localhost:3030/vie-dark/css/style.css" />

</head>

<body class="position-r">



    <!-- ==================== Start Loading ==================== -->

    <div class="loading">
        <span>L</span>
        <span>o</span>
        <span>a</span>
        <span>d</span>
        <span>i</span>
        <span>n</span>
        <span>g</span>
    </div>

    <div id="preloader">
    </div>

    <!-- ==================== End Loading ==================== -->

    

    <!-- ==================== Start cursor ==================== -->

    <div class="mouse-cursor cursor-outer"></div>
    <div class="mouse-cursor cursor-inner"></div>

    <!-- ==================== End cursor ==================== -->






    <div class="main-content">


 <!-- ==================== Start About ==================== -->

 <section class="about-ar section-padding">
	<div class="container">
		<div class="row">
			<div class="col-lg-4 valign">
				<div class="item-img">
                    <a href="" class="imago wow">
                        <img src="/image/item/<%= item.item_pic%>" alt="image">
                        <div class="item-img-overlay"></div>
                    </a>
                </div>
			</div>

			<div class="col-lg-8 valign">
				<section class="contact-sec style2 section-padding position-re bg-img" data-background="">
					<div class="container">
                        <div class="full-width">
							<h1 class="wow fadeIn"><%=item.item_name%></h1>
						</div>
						<div class="row justify-content-center">
                            <div class="form wow fadeInUp" >
                                <span class="tags main-color custom-font">
                                    <a>Begin Price: <%= item.item_begin_price%></a>
                                    <br>
                                    <a>Minimum Bid: <%= item.item_minimum_bid%></a>
                                    <br>
                                    <a>Minimum Sell: <%= item.item_minimum_sell%></a>
                                    </br>
                                  </span>
								<div class="controls">
                                    <a class="butn light mt-30 full-width" href="/auction/<%= user._id%>/<%= room._id%>/<%= item._id%>" > Join </a>
                                </div>
							</div>
						</div>
					</div>
				</section>
			</div>
		</div>
	</div>
</section>

<!-- ==================== End About ==================== -->






    </div>




